{
  "manifest_version": 3,
  "name": "ScryHub",
  "version": "0.1.0",
  "description": "Hub that adds local game store buttons to Scryfall via provider extensions.",
  "key":"",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://scryfall.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://scryfall.com/*"
      ],
      "resources": [
        "styles/shadow.css",
        "icons/scryhub.svg"
      ]
    }
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon@16.png",
    "32": "icons/icon@32.png",
    "48": "icons/icon@48.png",
    "128": "icons/icon@128.png"
  }
}