{
  "name": "scryhub",
  "private": true,
  "workspaces": [
    "packages/core", "packages/protocol", "packages/library-starter-pack"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.0",
    "tslib": "^2.8.1",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "npm run build -w @scryhub/protocol && npm run build -w @scryhub/core && npm run build -w @scryhub/library-starter-pack",
    "build:core": "npm run build -w @scryhub/core",
    "build:core-assets": "npm run build:assets -w @scryhub/core",
    "build:starter-pack": "npm run build -w @scryhub/library-starter-pack",
    "package:core": "npm run package -w @scryhub/core",
    "package:starter-pack": "npm run package -w @scryhub/library-starter-pack",
    "clean": "rimraf packages/*/dist packages/*/build"
  }
}
